		"lint": "eslint --ext .js --ext .ts --ext .tsx src/",
		"lint:fix": "eslint --fix --ext .js --ext .ts --ext .tsx ./src/",
		"android": "ENVFILE=.env.qa react-native run-android --variant=DevDebug",
		"windows:android": "set ENVFILE=.env.next react-native run-android --variant=DevDebug",
		"ios": "react-native run-ios",
		"build": "cd android && ENVFILE=.env.alpha && ./gradlew assembleAlphaRelease",
		"windows:build": "cd android && set ENVFILE=.env.alpha && gradlew assembleAlphaRelease",
		"build:next": "cd android && ENVFILE=.env.next && ./gradlew assembleNextRelease",
		"windows:build:next": "cd android && set ENVFILE=.env.next && gradlew assembleNextRelease",
		"build:qa": "cd android && ENVFILE=.env.qa && ./gradlew assembleQaRelease",
		"windows:build:qa": "cd android && set ENVFILE=.env.qa && gradlew assembleQaRelease",
		"debugBuild": "cd android && ./gradlew assembleDebug"
